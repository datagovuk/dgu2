<body>
  <script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>

  <div id="skiplink-container">
    <div>
      <a href="#content" class="skiplink">Skip to main content</a>
    </div>
  </div>

  <div id="global-cookie-message"></div>

  <header role="banner" id="global-header" class="with-proposition">
    <div class="header-wrapper">
      <div class="header-global">
        <div class="header-logo">
          <a href="/" title="" id="logo" class="content">
          data.gov.uk
          </a>
        </div>
      </div>
    </div>
  </header>

<main id="content" role="main">

<div class="phase-banner-alpha">
    <p>
      <strong class="phase-tag">Prototype</strong>
      <span>This is a new service – your <a href="#">feedback</a> will help us to improve it.</span>
    </p>
</div>

<div class="login-bar">
  <%= if logged_in?(@conn) do %>
    <ul class="login-info">
      <li>Logged in as <%= current_user(@conn).username %></li>
      <li><%= link "Sign out", to: session_path(@conn, :logout) %></li>
    </ul>
  <% end %>
</div>

<%= render @view_module, @view_template, assigns %>

</main>

<footer id="footer" role="contentinfo">
  <div class="footer-wrapper">
      <div class="footer-links">
        <%= if not logged_in?(@conn) do %>
          <ul>
            <li><%= link "Sign in",  to: session_path(@conn, :login_view) %></li>
            <li><%= link "Register", to: session_path(@conn, :new) %></li>
          </ul>
        <% end %>
      </div>
      <div class="copyright-and-licence">
        <a href="http://www.nationalarchives.gov.uk/information-management/re-using-public-sector-information/copyright-and-re-use/crown-copyright/"><img src="<%= static_path(@conn, "/govuk_template/assets/stylesheets/images/open-government-licence.png") %>" alt="Crown Copyright"></a><a href="https://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/" rel="license"><img src="<%= static_path(@conn, "/govuk_template/assets/stylesheets/images/govuk-crest.png") %>" alt="Open Government Licence"></a>
      </div>
    </div>
</footer>

</body>
  <script src="<%= static_path(@conn, "/js/app.js") %>" type="text/javascript"></script>
