<h2>Show publisher</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @publisher.name %>
  </li>

  <li>
    <strong>Title:</strong>
    <%= @publisher.title %>
  </li>

  <li>
    <strong>Url:</strong>
    <%= @publisher.url %>
  </li>

</ul>

<%= link "Edit", to: publisher_path(@conn, :edit, @publisher) %>
<%= link "Back", to: publisher_path(@conn, :index) %>
