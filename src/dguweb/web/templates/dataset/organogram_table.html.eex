<h1 class="heading-large">Organogram - <%= @type %></h1>

<table>
    <thead>
        <tr>
            <%= for header <- @organogram_header do %>
            <th><%= header %></th>
            <%= end %>
        </tr>
    </thead>
    <tbody>
        <%= for row <- @organogram_data do %>
        <tr>
            <%= for cell <- row do %>
            <td><%= cell %></td>
            <%= end %>
        </tr>
        <%= end %>
    </tbody>
</table>